{
    "extends": "airbnb",
        "rules": {
            "semi": [
                2,
                "never"
            ],
            "no-unexpected-multiline": 2,
            "linebreak-style": [0, "unix"],
            "quotes": [
                "error",
                "double"
            ],
            "no-console": 0,
            "jsx-a11y/href-no-hash": 0,
            "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }]
        }
}
